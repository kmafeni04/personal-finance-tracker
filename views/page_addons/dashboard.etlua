<div class="dashboard">
  <link rel="stylesheet" href="../../static/CSS/dashboard.css" />
  <div class="top">
    <h2>Dashboard</h2>
    <h4>Welcome, <%= Username or 'User' %></h4>
  </div>
  <div class="bottom">
    <div class="first-row">
      <% for i = 1,3 do %>
      <div
        hx-get="/bills_card<%= i %>"
        hx-swap="outerHTML"
        hx-trigger="load"
      ></div>
      <% end %>
    </div>
    <div class="second-row"><h1>Second set of cards</h1></div>
  </div>
</div>
